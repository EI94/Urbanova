{
  "name": "urbanova",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npx next dev -p 3112",
    "build": "SKIP_ENV_VALIDATION=true npx next build",
    "start": "npx next start -p 3112",
    "typecheck": "tsc -p . --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:rules": "npx tsx scripts/migrate.ts migrate:validate",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --watchAll=false",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare-deploy": "npm run typecheck && npm run lint && npm run lint:rules && npm run test:ci && npm run build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "vercel-deploy": "vercel",
    "vercel-build": "npm run build",
    "workspace:build": "npm run build --workspaces",
    "workspace:typecheck": "npm run typecheck --workspaces",
    "workspace:test": "npm run test --workspaces",
    "migrate:dry-run": "tsx scripts/migrate.ts migrate:dry-run",
    "migrate:apply": "tsx scripts/migrate.ts migrate:apply",
    "migrate:validate": "tsx scripts/migrate.ts migrate:validate",
    "phase1:audit": "tsx scripts/phase1-execution.ts --audit-only",
    "phase1:validate-sources": "tsx scripts/phase1-execution.ts --validate-sources-only",
    "phase1:monitor": "tsx scripts/phase1-execution.ts --monitor-only",
    "phase1:execute": "tsx scripts/phase1-execution.ts",
    "phase1:test": "tsx scripts/test-phase1.ts",
    "phase1:full": "npm run phase1:test && npm run phase1:execute"
  },
  "dependencies": {
